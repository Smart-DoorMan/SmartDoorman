<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#95989898">

    <Button
        android:id="@+id/btnDustClose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="172dp"
        android:background="#150B0B"
        android:text="CLOSE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/tvDeliveryTitle"
        android:layout_width="184dp"
        android:layout_height="93dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="144dp"
        android:autoText="true"
        android:gravity="center"
        android:text="날씨"
        android:textSize="36sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginEnd="275dp"
        android:autoText="true"
        android:gravity="center"
        android:textSize="28sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginEnd="275dp"
        android:autoText="true"
        android:gravity="center"
        android:textSize="28sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginEnd="271dp"
        android:autoText="true"
        android:gravity="center"
        android:textSize="28sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:layout_marginStart="231dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="35dp"
        android:autoText="true"
        android:gravity="center"
        android:text="최고기온"
        android:textSize="28sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toStartOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="147dp"
        android:layout_height="57dp"
        android:layout_marginStart="231dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="37dp"
        android:autoText="true"
        android:gravity="center"
        android:text="날씨"
        android:textSize="28sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvDeliveryTitle" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="120dp"
        android:layout_height="60dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="275dp"
        android:layout_marginBottom="8dp"
        android:autoText="true"
        android:gravity="center"
        android:textSize="28sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.735" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:layout_marginStart="231dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="37dp"
        android:autoText="true"
        android:gravity="center"
        android:text="현재기온"
        android:textSize="28sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:layout_marginStart="231dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="182dp"
        android:autoText="true"
        android:gravity="center"
        android:text="최저기온"
        android:textSize="28sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/btnDustClose"
        app:layout_constraintEnd_toStartOf="@+id/textView11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

</androidx.constraintlayout.widget.ConstraintLayout>